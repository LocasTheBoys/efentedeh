if game.PlaceId == 15860930970 then
    wait(1)
    -- Pilih Level
    local args = {
        [1] = "1",
        [2] = 1,
        [3] = 1
    }
    game:GetService("ReplicatedStorage").Remotes.Main.Vote:FireServer(unpack(args))

    --Pojokan Depan Pintu Masuk

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function getPart()
    -- Attempt to get the part from the specified location
    local part = workspace:FindFirstChild("Map") and workspace.Map:FindFirstChild("Paths"):FindFirstChild("1"):FindFirstChild("1")
    return part
end

for i = 1, 5 do -- Loop 5 times
    local partFound = false -- Flag to track if the part is found

    while true do
        -- Check if the map exists before attempting to get the part
        if workspace:FindFirstChild("Map") then
            local part = getPart()

            -- Check if the part is detected
            if part then
                partFound = true -- Set flag to true if part is found

                -- Teleport the character to the part's position if not already there
                if (humanoidRootPart.Position - part.Position).magnitude > 0.1 then -- Check if not already at part
                    humanoidRootPart.CFrame = CFrame.new(part.Position)
                end
            else
                if not partFound then
                    print("Waiting for part to be detected...") -- Log message when part is not found
                end
            end
        else
            print("Waiting for the map to load...") -- Log message when map is not loaded
        end

        wait(1) -- Wait for a second before checking again
    end

    wait(5) -- Wait for 5 seconds before the next loop iteration
end
    print("Game ID cocok, script dijalankan.")
else
    -- Script ini hanya dijalankan jika game ID tidak cocok
    print("Game ID tidak cocok, script tidak dijalankan.")
end

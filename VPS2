-- Loop untuk menjalankan 1000 kali
for i = 1, 1000 do
    local args = {
        [1] = 10  -- dan 1
    }

    -- Memanggil server menggunakan InvokeServer dengan argumen yang diberikan
    game:GetService("ReplicatedStorage").Remotes.Summons.Hatch:InvokeServer(unpack(args))
    
    wait(0.1)  -- Tambahkan sedikit jeda agar tidak terlalu cepat (opsional)
end

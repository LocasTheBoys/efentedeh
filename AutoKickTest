if game.PlaceId == 15860930970 then
    wait(30)
    print("Auto Kick Script Is Running")

    -- Fungsi untuk kick player
    local function kickPlayer(player, duration)
        wait(duration)
        player:Kick("You have been kicked from the game for 5 minutes.")
    end

    -- Durasi kick (5 menit = 300 detik)
    local kickDuration = 5 * 60

    -- Mendapatkan player dan melakukan kick
    local Players = game:GetService("Players")
    local localPlayer = Players.LocalPlayer
    kickPlayer(localPlayer, kickDuration)

    -- Menunggu prompt kick muncul
    repeat wait() until game.CoreGui:FindFirstChild('RobloxPromptGui')

    -- GPU Saver script setelah prompt kick
    print("GPU SAVER")
    local rs = game:GetService("RunService")
    local coreGui = game:GetService("CoreGui")

    -- Fungsi untuk menambahkan overlay hitam penuh ke layar
    local function addFullBlackScreen()
        if not coreGui:FindFirstChild("BlackOverlay") then
            local screenGui = Instance.new("ScreenGui")
            screenGui.Name = "BlackOverlay"
            screenGui.ResetOnSpawn = false
            screenGui.IgnoreGuiInset = true
            screenGui.DisplayOrder = 10000

            local blackFrame = Instance.new("Frame")
            blackFrame.Size = UDim2.new(1, 0, 1, 0)
            blackFrame.Position = UDim2.new(0, 0, 0, 0)
            blackFrame.BackgroundColor3 = Color3.new(0, 0, 0)
            blackFrame.BackgroundTransparency = 0
            blackFrame.BorderSizePixel = 0
            blackFrame.ZIndex = 10000
            blackFrame.Parent = screenGui

            screenGui.Parent = coreGui
        end
    end

    -- Nonaktifkan rendering 3D untuk mengurangi beban GPU
    rs:Set3dRenderingEnabled(false)

    -- Menambahkan overlay hitam penuh ke layar
    addFullBlackScreen()

    -- Mengaktifkan throttle untuk membatasi framerate
    rs:SetThrottleFramerateEnabled(true)

    print("Game ID cocok, GPU Saver diaktifkan.")
else
    print("Game ID tidak cocok, script tidak dijalankan.")
end
